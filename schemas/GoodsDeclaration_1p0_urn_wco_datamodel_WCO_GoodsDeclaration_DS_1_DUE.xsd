		<xs:enumeration value="DBII"><xs:annotation><xs:documentation>Drawback Isenção Intermediário</xs:documentation></xs:annotation></xs:enumeration>

		
		<xs:enumeration value="LPCO"><xs:annotation><xs:documentation>LPCO</xs:documentation></xs:annotation></xs:enumeration>
		
		<!-- Reexportação e Substituição de Mercadoria Importada (Enquadramento tipo indenização) -->
		<xs:enumeration value="DSI"><xs:annotation><xs:documentation>Declaração Simplificada de Importação</xs:documentation></xs:annotation></xs:enumeration>
		<xs:enumeration value="DSIF"><xs:annotation><xs:documentation>Declaração Simplificada de Importação - Formulário</xs:documentation></xs:annotation></xs:enumeration>
		<xs:enumeration value="DI"><xs:annotation><xs:documentation>Declaração de Importação</xs:documentation></xs:annotation></xs:enumeration>
		<xs:enumeration value="EDBV"><xs:annotation><xs:documentation>e-DBV</xs:documentation></xs:annotation></xs:enumeration>
		<xs:enumeration value="DUIMP"><xs:annotation><xs:documentation>DUIMP</xs:documentation></xs:annotation></xs:enumeration>
		
